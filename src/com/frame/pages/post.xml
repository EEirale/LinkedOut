<?xml version="1.0"?>
<main layout="GridBag">
    <object class="label" id="userLabel" size="240x60" type="dynamic">
        <constraints>
            <gridx>1</gridx>
            <gridy>0</gridy>
            <insets>0,0,0,0</insets>
        </constraints>
        <text>user</text>
    </object>
    <object class="label" id="dateLabel" size="240x60" type="dynamic">
        <constraints>
            <gridx>2</gridx>
            <gridy>0</gridy>
            <insets>0,0,0,0</insets>
        </constraints>
        <text>publication date</text>
    </object>
    <object class="label" id="IDLabel" type="dynamic" style="display:false">
        <constraints>
            <gridx>2</gridx>
            <gridy>0</gridy>
            <insets>0,0,0,0</insets>
        </constraints>
        <text>IDLabel</text>
    </object>
    <object class="label" id="postLabel" style="background-color:light-gray;" size="650x650" type="dynamic">
        <constraints>
            <gridx>1</gridx>
            <gridy>1</gridy>
            <insets>0,0,0,0</insets>
        </constraints>
        <text>post body</text>
    </object>
    <object class="label" id="tagLabel" style="background-color:orange;" size="240x40" type="dynamic">
        <constraints>
            <gridx>1</gridx>
            <gridy>2</gridy>
            <insets>0,0,0,0</insets>
        </constraints>
        <text>tag</text>
    </object>
</main>